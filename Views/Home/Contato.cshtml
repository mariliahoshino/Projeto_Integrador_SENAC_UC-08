@model Contato

@{
  ViewData["Title"] = "Contato";
}


<body>

  <main>


    <!--
        <input type="text" name="pesquisa" size="40" maxlenght="40" value="Digite o que deseja pesquisar">
        <a href="#"><button class="button button1">Pesquisar</button></a> <br>
-->


    <!--principais noitícias -->
    <div class="container">
      <!---->
      <section id="destaques" class="row">
        <!---->
        <!--destaque principal-->
        <div id="maior" class=" col-xs-12 col-sm-12 col-md-12 col-lg-6">


          <script src="http://localhost:5000/js/js.js"></script>
          

          <!--<form method="POST" asp-action="Cadastro">-->
          <form action="" method="POST" name="formulario" onsubmit="return validar(this)">
          <!--<form method="POST" asp-action="Contato" name="formulario" onsubmit="return validar(this)">-->
            <span asp-for class="msg-erro msg-nome"></span>
            <label for="Nome">Nome:</label> <br>
            <input type="text" id="Mome" name="Nome" required><br>

            <span class="msg-erro msg-email"></span>
            <label for="Email">E-mail:</label> <br>
            <input type="text" id="Email" name="Email"  required> <!--onkeyup="return validar(this)"-->
            <!--<button type='submit' id='validate'>Validate!</button> --> <a id='result'></a> <br>

            <label for="Telefone">Telefone:</label> <br>
            <input type="text" id="Telefone" name="Telefone" value="+55(" onkeyup="FormataTelefone(this)"> <br>

            <label for="Assunto"> Assunto: </label> <br>

            <textarea id="Mensagem" name="Mensagem" rows="10" cols="50" maxlength="500"></textarea> <br>

            <input type="checkbox" id="Queremkt" name="Queremkt">
            <label for="Queremkt">Deseja receber e-mails de produtos e promoções?</label>

            <input type="submit" value="Subscrever" class="button" onclick="cadastro()">
          </form>

          




        </div>
        <!--destaques secundários-->
        <div id="menor1" class="col-xs-12 col-sm-6 col-md-6 col-lg-6">
          <!---->

          <style>
            iframe {
              width: 100%;
              height: 100%;
            }
          </style>


          <iframe
            src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d1011.7520569004522!2d-46.30603980921563!3d-23.535492070119144!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x0%3A0x330d4da16e5adfed!2sDelta%20Moto!5e0!3m2!1spt-BR!2sbr!4v1621973906410!5m2!1spt-BR!2sbr"
            style="border:0;" allowfullscreen="" loading="lazy"></iframe>



        </div>


      </section>
    </div>
    <!---->









    <p>
      @ViewBag.Mensagem
    </p>



    <!-- adicionei isso-->
    <!--
        <script src="js.js"></script>
    -->

    


  </main>

</body>
